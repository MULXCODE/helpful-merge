{
  "name": "helpful-merge",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "browser": "./dist/index.js",
  "scripts": {
    "test:unit": "mocha -r ts-node/register ./test/unit.ts",
    "test": "npm run test:unit",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "watch": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "cover": "nyc mocha ./test/unit.ts"
  },
  "nyc": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
        ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/**.test.ts",
      "dist",
      "coverage",
      "src/Interfaces"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
